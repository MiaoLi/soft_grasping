<robot name="simple_oven">
  <link name="my_oven">
    <inertial>
      <mass value="0.1" />
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
    </inertial>

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius='0.15' length='0.1'/>
      </geometry>
    </visual>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <cylinder radius='0.15' length='0.1'/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="my_oven">
    <material>Gazebo/Blue</material>
  </gazebo>
</robot>
